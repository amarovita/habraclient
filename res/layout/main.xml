<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <ScrollView android:layout_width="match_parent" android:id="@+id/scrollView1" android:layout_height="match_parent">
        <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout1" android:layout_height="match_parent" android:orientation="vertical">
        	<WebView android:id="@+id/result" android:layout_width="match_parent" android:layout_height="match_parent"/>
        	<HorizontalScrollView android:id="@+id/horizontalScrollView1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        		<LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout3" android:layout_height="match_parent" android:orientation="horizontal"></LinearLayout>
        	</HorizontalScrollView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
